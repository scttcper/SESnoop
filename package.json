{
  "name": "sesnoop",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/scttcper/sesnoop"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "pnpm run build && vite preview",
    "deploy": "pnpm run build && pnpm run db:migrations:apply && wrangler deploy",
    "typecheck": "tsgo --build",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:seed": "wrangler d1 execute sesnoop --local --file worker/db/seed.sql",
    "db:migrations:generate": "drizzle-kit generate",
    "db:migrations:apply": "wrangler d1 migrations apply DB --remote",
    "test": "vitest",
    "lint": "oxlint . && oxfmt --check",
    "lint:fix": "oxlint . --fix && oxfmt"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@fontsource-variable/inter": "^5.2.8",
    "@hono/zod-openapi": "^1.2.0",
    "@libsql/client": "^0.17.0",
    "@scalar/hono-api-reference": "^0.9.38",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.16",
    "@tanstack/zod-adapter": "^1.157.16",
    "@tsndr/cloudflare-worker-jwt": "^3.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.10",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "recharts": "3.7.0",
    "shadcn": "^3.7.0",
    "sonner": "^2.0.7",
    "stoker": "^2.0.1",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.22.1",
    "@cloudflare/vitest-pool-workers": "^0.12.8",
    "@cloudflare/workers-types": "^4.20260130.0",
    "@ctrl/oxlint-config": "^1.3.3",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260130.1",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vitest": "^3.2.4",
    "wrangler": "^4.61.1"
  },
  "packageManager": "pnpm@10.28.2"
}
